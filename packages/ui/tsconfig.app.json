{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "types": [ "@types/node", "@testing-library/jest-dom"],
    /* Bundler mode */
    "moduleResolution": "bundler",
    "allowImportingTsExtensions": true,
    "isolatedModules": true,
    "moduleDetection": "force",
    "noEmit": true,
    "jsx": "react-jsx",
  },
  "include": ["src"]
}
